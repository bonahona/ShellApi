{
  "Application": {
    "DefaultLayout": "Default",
    "NotFoundController" : "Projects",
    "NotFoundAction": "NotFound",
    "CapitalizeControllerName": true,
    "CapitalizeActionName": true
  },
  "Debug":{
    "DieOnRoutingError": true,
    "DontCacheModels": false
  },
  "Logging": {
    "FileDirectory": "/logs/",
    "Loggers": [
      {
        "CacheLogger": {
          "Name": "Cache"
        }
      }
    ]
  },
  "Caching":{
    "FileOutputCache":{
      "Name": "OutputCache",
      "StorageTime": 3600,
      "CacheFolder": "/Temp/Cache/Output/"
    }
  }
}